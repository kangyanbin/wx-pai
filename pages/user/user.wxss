@import '../../utils/font.wxss';
/* pages/user/user.wxss */
.page4 .mePic {
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction:column;
  background: #1e90ff;
}
.page4 .mePic image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}
.page4 .mePic .user{
  padding: 10px;
}
.page4 .mePag>view {
  padding: 32rpx 40rpx;
  background: white;
  font-size: 32rpx;    
  display: flex;
  justify-content: space-between;
  align-items: center;  
}
.page4 .meMap {
  border-top: 2rpx #f2f2f2 solid;
}
.page4 .mePag .meMore {
  font-size: 44rpx;
}
.calling{
  width: 100%;
  margin-top: 30rpx;
  /* height: 150rpx; */
  /* background-color: gray; */
}
.calling .box{
  display: inline-block;
  width: 150rpx;
  /* height: 140rpx; */
  text-align: center;
}
.calling .box image{
  width: 100rpx;
  height: 100rpx;
  color: #ffffff;
}